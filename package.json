{
  "name": "strategic-investments",
  "version": "1.0.0",
  "description": "Χρησιμοποιεί το API του Προγράμματος Δι@υγεια για την εύρεση στρατηγικών επενδύσεων, και συγκεκριμένα πράξεις χορήγησης κινήτρων, και την εξαγωγή δεδομένων.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "collect": "node --loader ts-node/esm src/scripts/collect-data.ts",
    "collect:with-api": "node --loader ts-node/esm src/scripts/collect-data.ts --apiKey $npm_config_apiKey",
    "collect:period": "node --loader ts-node/esm src/scripts/collect-data.ts --startDate $npm_config_startDate --endDate $npm_config_endDate",
    "collect:period:with-api": "node --loader ts-node/esm src/scripts/collect-data.ts --startDate $npm_config_startDate --endDate $npm_config_endDate --apiKey $npm_config_apiKey",
    "start": "TS_NODE_PROJECT=tsconfig.web.json webpack serve --config webpack.config.mjs --mode development --open",
    "build": "TS_NODE_PROJECT=tsconfig.web.json NODE_ENV=production webpack --config webpack.config.mjs --mode production",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "strategic-investments",
    "diavgeia",
    "greece",
    "transparency"
  ],
  "author": "",
  "license": "MIT",
