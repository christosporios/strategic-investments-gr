    "postcss-loader": "^8.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "style-loader": "^3.3.4",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.12",
